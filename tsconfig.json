{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": false,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "importHelpers": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react",
        "noImplicitAny": false,
        "baseUrl": ".",
        "paths": {
            "video-react-hooks/*": ["src/*"]
        }
    },
    "include": ["src", "public/index.tsx", "src/custom.d.ts"],
    "exclude": ["node_modules", "build"]
}
